{
  "name": "calendar",
  "version": "1.0.0",
  "status": "implemented",
  "description": "Apple Calendar integration for event management",
  "capabilities": [
    "list-calendars",
    "list-today",
    "list-week",
    "list-events",
    "create-event",
    "find-event",
    "modify-event",
    "delete-event",
    "check-conflicts",
    "add-alarm",
    "handle-notification"
  ],
  "dependencies": {
    "applescript": true,
    "icloud-storage": true
  },
  "notificationFilter": {
    "whitelist": {
      "content": ["\\[AGENT\\]", "\\[BROKKR\\]"]
    },
    "patterns": [
      { "match": "interview", "priority": "HIGH" },
      { "match": "deadline", "priority": "HIGH" }
    ],
    "blacklist": {
      "titles": ["^declined:", "^cancelled:"]
    }
  },
  "icloudPaths": {
    "exports": "Exports"
  }
}
